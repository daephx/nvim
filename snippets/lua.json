{
  "require": {
    "prefix": "req",
    "body": ["require(\"${1:module}\")"],
    "description": "Require module"
  },
  "return": {
    "prefix": "ret",
    "body": ["return $0"],
    "description": "return value"
  },
  "M": {
    "prefix": "M",
    "body": ["local M = {}\n$0\nreturn M"]
  },
  "local": {
    "prefix": "l",
    "body": ["local ${0}"],
    "description": "create a variable"
  },
  "if": {
    "prefix": "if",
    "body": ["if ${1:true} then", "\t$0", "end"]
  },
  "elseif": {
    "prefix": "elseif",
    "body": ["elseif ${1:true} then", "\t$0"]
  },
  "for": {
    "prefix": "for",
    "body": ["for ${1:i}=${2:1},${3:10} do", "\t$0", "end"],
    "description": "for loop range"
  },
  "function": {
    "prefix": "func",
    "body": ["function ${1:name}($2)", "\t${3:return}", "end"]
  },
  "local function": {
    "prefix": "lfunc",
    "description": "Creates an anonymous function",
    "body": ["local $1 = function($2) $0 end"]
  },
  "print": {
    "prefix": "pr",
    "body": ["print(${0})"]
  },
  "while": {
    "prefix": "while",
    "body": ["while ${1:true} do", "\t$0", "end"]
  },
  "vimapi": {
    "prefix": "api",
    "body": ["local api = vim.api"]
  },
  "vimfn": {
    "prefix": "fn",
    "body": ["local fn = vim.fn"]
  },
  "vimlsp": {
    "prefix": "lsp",
    "body": ["local lsp = vim.lsp"]
  }
}
