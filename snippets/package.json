{
  "name": "nvim-snippets",
  "author": "daephx",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "all",
          "global",
          "html",
          "markdown",
          "plaintext",
          "tex"
