      {
        "language": [
          "markdown",
          "rmd"
        ],
        "path": "./markdown.json"
      },
      {
        "language": [
          "powershell",
          "ps1"
        ],
        "path": "./powershell.json"
      },
      {
        "language": "python",
        "path": "./python.json"
      },
      {
