# vim ft=snippets
# everything should override these. These shebangs are for inserting in
# files with no filetype. (So then vim can determine the filetype from the
# shebang.)
priority -1000

snippet #! 'sh shebang'
#!/usr/bin/env sh
endsnippet

snippet #! 'bash shebang'
#!/usr/bin/env bash
endsnippet

snippet #! 'zsh shebang'
#!/usr/bin/env zsh
endsnippet

snippet #! 'python shebang'
#!/usr/bin/env python
# -*- coding: utf-8 -*-
endsnippet
